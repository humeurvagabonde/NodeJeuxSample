<% if (request.loggedIn) { %>
	<ul id="menu">
	  <!-- put class="selected" in the li tag for the selected page - to highlight which page you're on -->
	  <li <% if (response.currentPage == 'accueil') { %> class='selected' <% } %>>
		<a href="/">Accueil</a>
	  </li>
	  <li <% if (response.currentPage == 'compte') { %> class='selected' <% } %>>
	    <a href="/compte">Mon Compte</a>
	  </li>
	  <li <% if (response.currentPage == 'jouer') { %> class='selected' <% } %>>
	    <a href="/jouer">Jouer</a>
	  </li>
	  <li>
	    <a href="/logout">Se déconnecter</a>
	  </li>
	</ul>
<% } else { %>
	<ul id="menu">
	  <!-- put class="selected" in the li tag for the selected page - to highlight which page you're on -->
	  <li <% if (response.currentPage == 'accueil') { %> class='selected' <% } %>>
	    <a href="/">Accueil</a>
	  </li>
	  <li <% if (response.currentPage == 'compte') { %> class='selected' <% } %>>
	    <a href="/compte">Mon Compte</a>
	  </li>
	  <li <% if (response.currentPage == 'jouer') { %> class='selected' <% } %>>
	    <a href="/jouer">Jouer</a>
	  </li>
	</ul>
<% } %>